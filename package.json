{
  "name": "nativeapp",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "ios:clean": "yarn clean; yarn ios",
    "start": "react-native start",
    "xcode": "xed -b ios",
    "test": "jest",
    "build": "yarn build:android && yarn build:ios",
    "build:ios": "./buildIOSRelease.sh",
    "build:android": "./buildApkRelease.sh",
    "postinstall": "patch-package; yarn buildChart",
    "buildChart": "$(git rev-parse --show-toplevel)/buildChart.sh",
    "clean:ios": "xcodebuild clean -workspace ./ios/nativeapp.xcworkspace -scheme nativeapp",
    "repod": "cd ios; pod deintegrate; pod install",
    "clean": "yarn clean:ios; yarn repod",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "react-native": "react-native"
  },
  "dependencies": {
    "@react-native-clipboard/clipboard": "^1.9.0",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/native": "^6.0.4",
    "@react-navigation/native-stack": "^6.2.2",
    "@react-navigation/stack": "^6.0.9",
    "@sentry/react-native": "^3.2.13",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/lodash.zip": "^4.2.6",
    "@types/lodash.zipobject": "^4.1.6",
    "@types/react-native-svg-charts": "^5.0.12",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "is_js": "^0.9.0",
    "lodash.get": "^4.4.2",
    "lodash.omit": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "lodash.zip": "^4.2.0",
    "lodash.zipobject": "^4.1.3",
    "meta1dex": "^0.6.13",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "qrcode": "^1.4.4",
    "react": "17.0.2",
    "react-native": "0.66.0",
    "react-native-encrypted-storage": "^4.0.2",
    "react-native-feather": "^1.1.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-inappbrowser-reborn": "^3.6.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.4",
    "react-native-paper": "^4.9.2",
    "react-native-prompt-android": "^1.1.0",
    "react-native-reanimated": "^2.2.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.1",
    "react-native-svg-charts": "^5.4.0",
    "react-native-text-input-mask": "^3.1.4",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": "^11.14.2",
    "stream-browserify": "^3.0.0",
    "zustand": "^3.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/qrcode": "^1.4.1",
    "@types/react-native": "^0.65.0",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "17.0.2",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
