  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --device",
    "ios:simulator": "react-native run-ios --simulator=\"iPhone 15 Pro\"",
    "ios:plus": "react-native run-ios --simulator=\"iPhone 15 Plus\"",
    "ios:SE": "react-native run-ios --simulator=\"iPhone SE (1st generation)\"",
    "ios:clean": "yarn clean; yarn ios",
    "start": "react-native start",
    "xcode": "xed -b ios",
    "test": "jest",
    "build": "yarn build:android && yarn build:ios",
    "build:ios": "cd ios; fastlane nightly --env '.env'",
    "build:android": "./buildApkRelease.sh",
    "postinstall": "patch-package; yarn buildChart",
    "buildChart": "$(git rev-parse --show-toplevel)/buildChart.sh",
    "clean:ios": "xcodebuild clean -workspace ./ios/nativeapp.xcworkspace -scheme nativeapp",
    "clean:android": "cd android && ./gradlew clean",
    "repod": "cd ios; pod deintegrate; pod install",
    "clean": "yarn clean:android; yarn clean:ios; yarn repod",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "react-native": "react-native",
    "bump": "GIT_HEAD=+$(git rev-parse --short HEAD) bundle exec fastlane bump",
    "bump:testnet": "GIT_HEAD=+$(git rev-parse --short HEAD)-testnet bundle exec fastlane bump",
    "build:testnet": "yarn bump:testnet && yarn clean && yarn testnet:android && yarn testnet:ios && git reset --hard HEAD",
    "testnet:android": "NETWORK=TESTNET ./buildApkRelease.sh",
    "testnet:ios": "export NETWORK=TESTNET; yarn build:ios"
  },
